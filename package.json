{
  "name": "pg-sql-helpers",
  "description": "A set helpers for writing dynamic SQL queries with `pg-sql` in Javascript.",
  "version": "0.1.4",
  "license": "MIT",
  "repository": "git://github.com/ianstormtaylor/pg-sql-helpers.git",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./lib",
    "clean": "rm -rf ./lib ./node_modules",
    "lint": "eslint src/* test/*",
    "prepublish": "yarn run build",
    "release": "np",
    "test": "yarn run build && mocha --compilers js:babel-core/register ./test/index.js",
    "watch": "babel ./lib --out-dir ./lib --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.8.1",
    "eslint-plugin-import": "^2.0.1",
    "is-email": "^1.0.0",
    "mocha": "^3.2.0",
    "np": "^2.13.1"
  },
  "keywords": [
    "dynamic",
    "escape",
    "interpolate",
    "orm",
    "pg",
    "pg-sql",
    "pgsql",
    "postgres",
    "postgresql",
    "query",
    "safe",
    "sql",
    "string",
    "template"
  ],
  "dependencies": {
    "custom-err": "^0.0.2",
    "is": "^3.2.1",
    "is-plain-object": "^2.0.4",
    "lodash": "^4.17.4",
    "pg-sql": "^1.1.0"
  }
}
